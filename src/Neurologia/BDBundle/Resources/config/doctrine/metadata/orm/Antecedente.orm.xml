<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Antecedente" table="antecedente">
    <indexes>
      <index name="FK_antecedente" columns="tipo_antecedente_id"/>
      <index name="FK_antecedente2" columns="historia_clinica_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descripcion" type="string" column="descripcion" length="255" nullable="false"/>
    <many-to-one field="tipoAntecedente" target-entity="TipoAntecedente">
      <join-columns>
        <join-column name="tipo_antecedente_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="historiaClinica" target-entity="HistoriaClinica">
      <join-columns>
        <join-column name="historia_clinica_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

{% extends 'NeurologiaGenericosBundle:Default:abm.html.twig' %}

{% block container %}
    <h1>TipoEstudio</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
            <tr>

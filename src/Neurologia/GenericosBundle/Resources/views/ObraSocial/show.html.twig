{% extends 'NeurologiaGenericosBundle:Default:abm.html.twig' %}

{% block container %}
    <h1>ObraSocial</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('obrasocial') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('obrasocial_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}

<?php

namespace Neurologia\FirstBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * EfectoAdverso
 *
 * @ORM\Table(name="efecto_adverso")
 * @ORM\Entity
 */
class EfectoAdverso
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id_efecto_adverso", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $idEfectoAdverso;

    /**
     * @var string
     *
     * @ORM\Column(name="descripcion", type="string", length=255, nullable=false)
     */
    private $descripcion;



    /**
     * Get idEfectoAdverso
     *
     * @return integer 
     */
    public function getIdEfectoAdverso()
    {
        return $this->idEfectoAdverso;
    }

    /**
     * Set descripcion
     *
     * @param string $descripcion
     * @return EfectoAdverso
     */
    public function setDescripcion($descripcion)
    {
        $this->descripcion = $descripcion;

        return $this;
    }

    /**
     * Get descripcion
     *
     * @return string 
     */
    public function getDescripcion()
    {
        return $this->descripcion;
    }
}

<?php

namespace Neurologia\FirstBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * TipoAntecedente
 *
 * @ORM\Table(name="tipo_antecedente")
 * @ORM\Entity
 */
class TipoAntecedente
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id_tipo_antecedente", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $idTipoAntecedente;

    /**
     * @var string
     *
     * @ORM\Column(name="descripcion", type="string", length=255, nullable=false)
     */
    private $descripcion;



    /**
     * Get idTipoAntecedente
     *
     * @return integer 
     */
    public function getIdTipoAntecedente()
    {
        return $this->idTipoAntecedente;
    }

    /**
     * Set descripcion
     *
     * @param string $descripcion
     * @return TipoAntecedente
     */
    public function setDescripcion($descripcion)
    {
        $this->descripcion = $descripcion;

        return $this;
    }

    /**
     * Get descripcion
     *
     * @return string 
     */
    public function getDescripcion()
    {
        return $this->descripcion;
    }
}

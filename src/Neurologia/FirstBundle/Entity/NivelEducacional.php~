<?php

namespace Neurologia\FirstBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * NivelEducacional
 *
 * @ORM\Table(name="nivel_educacional")
 * @ORM\Entity
 */
class NivelEducacional
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id_nivel_educacional", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $idNivelEducacional;

    /**
     * @var string
     *
     * @ORM\Column(name="descripcion", type="string", length=255, nullable=false)
     */
    private $descripcion;



    /**
     * Get idNivelEducacional
     *
     * @return integer 
     */
    public function getIdNivelEducacional()
    {
        return $this->idNivelEducacional;
    }

    /**
     * Set descripcion
     *
     * @param string $descripcion
     * @return NivelEducacional
     */
    public function setDescripcion($descripcion)
    {
        $this->descripcion = $descripcion;

        return $this;
    }

    /**
     * Get descripcion
     *
     * @return string 
     */
    public function getDescripcion()
    {
        return $this->descripcion;
    }
}

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Evolucion" table="evolucion">
    <indexes>
      <index name="id_historia_clinica" columns="id_historia_clinica"/>
      <index name="id_usuario" columns="id_usuario"/>
    </indexes>
    <id name="idEvolucion" type="integer" column="id_evolucion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="evolucion" type="string" column="evolucion" length="255" nullable="true"/>
    <field name="fechaHoraConsulta" type="datetime" column="fecha_hora_consulta" nullable="true"/>
    <many-to-one field="idHistoriaClinica" target-entity="HistoriaClinica">
      <join-columns>
        <join-column name="id_historia_clinica" referenced-column-name="id_paciente"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUsuario" target-entity="Usuario">
      <join-columns>
        <join-column name="id_usuario" referenced-column-name="id_persona"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

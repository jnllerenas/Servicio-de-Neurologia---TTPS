<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Interno" table="interno">
    <id name="idTratamiento" association-key="true"/>
    <field name="inicio" type="date" column="inicio" nullable="true"/>
    <field name="activo" type="boolean" column="activo" nullable="true"/>
    <one-to-one field="idTratamiento" target-entity="Tratamiento">
      <join-columns>
        <join-column name="id_tratamiento" referenced-column-name="id_tratamiento"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Antecedente" table="antecedente">
    <indexes>
      <index name="id_historia_clinica" columns="id_historia_clinica"/>
      <index name="id_tipo_antecedente" columns="id_tipo_antecedente"/>
    </indexes>
    <id name="idAntecedente" type="integer" column="id_antecedente">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descripcion" type="string" column="descripcion" length="255" nullable="true"/>
    <many-to-one field="idHistoriaClinica" target-entity="HistoriaClinica">
      <join-columns>
        <join-column name="id_historia_clinica" referenced-column-name="id_paciente"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTipoAntecedente" target-entity="TipoAntecedente">
      <join-columns>
        <join-column name="id_tipo_antecedente" referenced-column-name="id_tipo_antecedente"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>

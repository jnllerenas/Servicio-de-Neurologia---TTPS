<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="DrogaTratamiento" table="droga_tratamiento">
    <indexes>
      <index name="id_droga" columns="id_droga"/>
      <index name="id_efecto_adverso" columns="id_efecto_adverso"/>
      <index name="IDX_F9CB63A71E8DD6F2" columns="id_interno"/>
    </indexes>
    <id name="dosis" type="string" column="dosis" length="255"/>
    <id name="idEfectoAdverso" association-key="true"/>
    <id name="idDroga" association-key="true"/>
    <id name="idInterno" association-key="true"/>
    <one-to-one field="idInterno" target-entity="Interno">
      <join-columns>
        <join-column name="id_interno" referenced-column-name="id_tratamiento"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idDroga" target-entity="Droga">
      <join-columns>
        <join-column name="id_droga" referenced-column-name="id_droga"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="idEfectoAdverso" target-entity="EfectoAdverso">
      <join-columns>
        <join-column name="id_efecto_adverso" referenced-column-name="id_efecto_adverso"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Motivo" table="motivo">
    <indexes>
      <index name="id_historia_clinica" columns="id_historia_clinica"/>
    </indexes>
    <id name="idMotivo" type="integer" column="id_motivo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="detalle" type="string" column="detalle" length="255" nullable="true"/>
    <many-to-one field="idHistoriaClinica" target-entity="HistoriaClinica">
      <join-columns>
        <join-column name="id_historia_clinica" referenced-column-name="id_paciente"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
